{
  "name": "@romasato/web-spa-primer",
  "author": "Romas Kriauciukas",
  "license": "ISC",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.tsx",
  "private": true,
  "scripts": {
    "spa:dev": "rm -rf dist && webpack --progress --mode=development --watch --config webpack.config.js",
    "spa:prod": "webpack --progress -p --config webpack.config.js",
    "server:dev": "ts-node-dev --project ./tsconfig.server.json --respawn --transpileOnly ./server/server.ts",
    "server:build": "tsc --project ./tsconfig.server.json",
    "clean:build": "rm -rf dist dist-server storybook-static",
    "dev": "npm run clean:build && npm run server:dev & npm run spa:dev",
    "build:all": "npm run clean:build && npm run spa:prod && npm run server:build",
    "start": "npm run build:all && node ./dist-server/server.js",
    "storybook": "npm run clean:build && start-storybook --ci -h localhost -p 9000 -s dist -c .storybook ",
    "storybook:build": "build-storybook -s www -c .storybook",
    "storybook:serve": "serve -l 9000 ./storybook-static"
  },
  "keywords": [
    "metronome",
    "music",
    "bpm",
    "coding_task"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reset-css": "^5.0.1",
    "respinner": "^3.0.2",
    "serve": "^11.3.0",
    "styled-components": "^5.0.1",
    "webpack": "^4.42.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@types/classnames": "^2.2.10",
    "@types/cors": "^2.8.6",
