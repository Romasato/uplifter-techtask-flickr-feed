    "@types/redux": "^3.6.0",
    "@types/serve-handler": "^6.1.0",
    "@types/styled-components": "^5.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "copy-webpack-plugin": "^5.1.1",
